<body>
<div>
	<table>
		<tr>
 		<td><b>utente</b></td>
 		<td><b>data e ora</b></td>
 		<td><b>prodotto</b></td>
 		<td><b>numero</b></td>
 	</tr>
 	<% orders.forEach(function(order){ %>
 	<tr>
 		<td><%=order.utente%></td>
		<td><%=new Date(order.dataeora).toString()%></td>
		<% order.prodotti.forEach(function(product){ %>
		<td><%=product.id%></td>
		<td><%=product.quantita%></td>
			<% });%></tr>
			<% });%>
	</table>
	
</div>

